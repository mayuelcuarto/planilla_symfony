<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sector" table="sector">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="anoEje" type="integer" column="ano_eje" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sector" type="string" column="sector" length="2" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="boolean" column="estado" nullable="false"/>
  </entity>
</doctrine-mapping>
