<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Plaza" table="plaza">
    <id name="tipoPlanilla" type="string" column="tipo_planilla" length="2"/>
    <id name="numPlaza" type="string" column="num_plaza" length="6"/>
    <field name="grupoOcupacional" type="string" column="grupo_ocupacional" length="2" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="categoriaOcupacional" type="string" column="categoria_ocupacional" length="2" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="secFunc" type="string" column="sec_func" length="4" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="estado" type="string" column="estado" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">A</option>
      </options>
    </field>
    <field name="especifica" type="string" column="especifica" length="10" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
